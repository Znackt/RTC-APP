{
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/index.js"
        }
    ]
}